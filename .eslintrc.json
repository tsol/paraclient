{
  "extends": ["airbnb", "prettier", "plugin:vue/essential"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "no-console": "off",
    "func-names": "off",
    "no-process-exit": "off",
    "object-shorthand": "off",
    "class-methods-use-this": "off"
  },
  "parserOptions": {
    "parser": "@babel/eslint-parser"
  }
}
